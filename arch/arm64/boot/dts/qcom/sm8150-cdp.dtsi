&qupv3_se17_i2c {
    status = "ok";
    focaltech@38 {
		compatible = "focaltech,fts";
		reg = <0x38>;
		interrupt-parent = <&tlmm>;
		interrupts = <122 0x2008>;
		focaltech,reset-gpio = <&tlmm 54 0x01>;
		focaltech,irq-gpio = <&tlmm 122 0x2008>;
		focaltech,max-touch-number = <10>;
		/* pins used by touchscreen */
		//pinctrl-names = "pmx_ts_active", "pmx_ts_suspend";
		//pinctrl-0 = <&ts_active>;
		//pinctrl-1 = <&ts_int_suspend &ts_reset_suspend>;
		//focaltech,name = "ft8719";
		vdd-supply = <&pm8150_l14>;
		lab-supply = <&lcdb_ldo_vreg>;
		ibb-supply = <&lcdb_ncp_vreg>;
		focaltech,regulator_dvdd = "vdd";
		focaltech,regulator_lab = "lab";
		focaltech,regulator_ibb = "ibb";
		focaltech,display-coords = <0 0 1080 2340>;
		focaltech,display-coords-ara = <0 0 1080 2246>;
	};
};

&qupv3_se9_i2c {
	status = "ok";
	nq@28 {
		compatible = "qcom,nq-nci";
		reg = <0x28>;
		qcom,nq-irq = <&tlmm 47 0x00>;
		qcom,nq-ven = <&tlmm 41 0x00>;
		qcom,nq-firm = <&tlmm 48 0x00>;
		qcom,nq-clkreq = <&tlmm 113 0x00>;
		qcom,nq-esepwr = <&tlmm 42 0x00>;
		interrupt-parent = <&tlmm>;
		interrupts = <47 0>;
		interrupt-names = "nfc_irq";
		pinctrl-names = "nfc_active", "nfc_suspend";
		pinctrl-0 = <&nfc_int_active &nfc_enable_active
					&nfc_clk_req_active>;
		pinctrl-1 = <&nfc_int_suspend &nfc_enable_suspend
					&nfc_clk_req_suspend>;
	};
};

